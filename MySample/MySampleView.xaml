<UserControl x:Class="Genesyslab.Desktop.Modules.Incom.MySample.MySampleView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="30" Width="345" MinWidth="20">
    <Grid x:Name="DispositionGrid" VerticalAlignment="Top">
        <StackPanel x:Name="VoiceStackPanel" Orientation="Horizontal" VerticalAlignment="Center" Height="30" Visibility="Visible" Loaded="VoiceStackPanel_Loaded">
            <Button x:Name="btn_start"	Content="Create/Verify"	Margin="0,0,10,0"  VerticalAlignment="Top" ClipToBounds="False" HorizontalContentAlignment="Center" FontStyle="Normal" Height="30" Click="btnBioStart_Click" />
            <Button x:Name="btnSubmit"	Content="Submit"  Margin="0,0,10,0" ClipToBounds="False" HorizontalContentAlignment="Center" FontStyle="Normal" Height="30" Click="BtnBioSubmit_Click" IsEnabled="False" />
            <Button x:Name="btnCancel"	Content="Cancel"  Margin="0,0,10,0" ClipToBounds="False" HorizontalContentAlignment="Center" FontStyle="Normal" Height="30" Click="btnBioCancel_Click" />
            <Ellipse x:Name="elps_bio" Height="30" Stroke="Black" Width="30" Fill="Gray"/>
            <Label x:Name="lblBioScore" Content="%%" Margin="10,0,10,0" Width="35" Height="30" FontSize="14" VerticalAlignment="Center"/>
            <ProgressBar Height="10" Width="85" Minimum="0"  Maximum="100" x:Name="pbStatus" Margin="10,0,0,0"/>
        </StackPanel>
    </Grid>
</UserControl>